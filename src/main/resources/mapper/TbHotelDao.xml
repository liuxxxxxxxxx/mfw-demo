<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.dao.TbHotelDao">
    <select id="listTbHotel" resultType="com.qf.pojo.po.TbHotel">
    select * from hotel
    limit #{offset},#{pageCount}
    </select>
    <select id="getCount" resultType="_int">
        select count(1) from
        hotel
    </select>
</mapper>